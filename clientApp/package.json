{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "buildDev": "vue-cli-service build --skip-plugins pwa,workbox --mode development",
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "preview": "npm run serve",
    "devBuild": "vite build --minify false",
    "buildClients": "npm run buildAuthClient && npm run buildIoClient && npm run buildMediaClient && npm run buildQueryClient",
    "buildAuthClient": "openapi --input ./src/services/auth/auth.yaml --output ./src/services/auth --name AuthAPI",
    "buildIoClient": "openapi --input ./src/services/io/io.yaml --output ./src/services/io --name IoAPI",
    "buildMediaClient": "openapi --input ./src/services/media/media.yaml --output src/services/media --name MediaAPI",
    "buildQueryClient": "openapi --input ./src/services/query/query.yaml --output ./src/services/query --name QueryAPI",
    "test": "jest",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "postinstall": "npm run buildClients"
  },
  "dependencies": {
    "@formkit/core": "^1.0.0-beta.12",
    "@formkit/pro": "^1.0.0-alpha.58",
    "@formkit/vue": "*",
    "@mdi/font": "^7.1.96",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.2.45",
    "@fontsource/roboto": "^4.5.8",
    "axios": "^1.2.2",
    "core-js": "^3.8.3",
    "datatables.net-dt": "^1.13.1",
    "datatables.net-responsive-dt": "^2.4.0",
    "datatables.net-vue3": "^2.0.0",
    "formkit": "*",
    "gravatar": "^1.8.2",
    "jwt-decode": "^3.1.2",
    "jwt-encode": "^1.0.1",
    "luxon": "^3.2.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.22.2",
    "register-service-worker": "^1.7.2",
    "uuid": "^9.0.0",
    "vee-validate": "^4.15.0",
    "vite": "^6.2.6",
    "vue": "^3.4",
    "vue-router": "^4.0.3",
    "vuetify": "^3.8.2",
    "vuex": "^4.0.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@formkit/themes": "^1.0.0-beta.12",
    "@types/jest": "^27.0.1",
    "@types/mocha": "^8.2.1",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "chromedriver": "108",
    "jest": "^27.0.5",
    "openapi-typescript-codegen": "^0.23.0",
    "sass": "^1.32.7",
    "ts-jest": "^27.0.4",
    "ts-node": "^9.1.1",
    "typescript": "~4.7",
    "vue-loader": "^17.4.2",
    "wdio-chromedriver-service": "^7.0.0"
  }
}
